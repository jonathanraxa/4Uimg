<!-- ALL THE PRICING INFORMATION FOR EACH LICENSE TYPE - MODALS -->


<!-- UNLIMITED MODAL -->
<div class="modal" id="unlimited">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">X</span></button> Unlimited
            </div>
            <div class="modal-body">
                Artwork licensed under an Unlimited license can be used in print form and in the Internet (World Wide Web).
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary pull-right" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


<!-- WEB MODAL -->
<div class="modal" id="web">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">X</span></button> Web
            </div>
            <div class="modal-body">
                Artwork licensed under a Web license can only be used in the Internet (World Wide Web). It may not be used in print form.
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary pull-right" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>


<!-- PRINT MODAL -->
<div class="modal" id="print">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">X</span></button> Print
            </div>
            <div class="modal-body">
                Artwork licensed under a Print license can only be used in the print form. It may not be used in electronic form, such as on the Internet (World Wide Web).
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary pull-right" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>